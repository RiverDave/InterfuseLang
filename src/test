# This is a function declaration.
fn @doMath(@a: int, @b: int) -> int {
    ret @a + @b * 2;
};

# # Main entry point(optional)
fn @main() -> int {
    @my_var = 45;
    @y : int = @doMath(@var, 23);

    ret 0;
};

# # range based for loops
# # increments by default
for @i in 0 := 10 {
    @i = @i + 1;
};

